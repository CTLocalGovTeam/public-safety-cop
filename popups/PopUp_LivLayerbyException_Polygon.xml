<configuration>

<!--	Pop-up for a feature layer using a free-flow description with HTML and multiple fields. -->
<!--<field>—Which fields to display (or not) and how to label and format them. -->
<!--    name—Name of the field to use. Required. Case-sensitive.-->
<!--    alias —Alias to display instead of the name. Optional.-->
<!--    visible—Boolean. Default value is false. Optional.-->

	<title>Name : {event}</title>
    <fields>
         <!-- Used in title, do not repeat in content -->
        <field name="objectid"  alias="OBJECTID" visible="true"/>  <!-- Overwrite alias specified in Map Service -->
        <field name="fips" alias="fips" visible="true"/>
        <field name="effective" alias="effective" visible="true"/>
        <field name="expires" alias="expires" visible="true"/>
    </fields>

</configuration>

